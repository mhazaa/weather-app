{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noUncheckedIndexedAccess": true,
		"exactOptionalPropertyTypes": true,
		"isolatedModules": true,
		"noUncheckedSideEffectImports": true,
		"moduleDetection": "force",
		"strict": true,
		"skipLibCheck": true,
	}
}